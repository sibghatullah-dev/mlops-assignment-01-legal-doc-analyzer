[flake8]
max-line-length = 120
max-complexity = 10
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    .env,
    build,
    dist,
    *.egg-info,
    .pytest_cache,
    node_modules,
    model/__pycache__,
    app/__pycache__
ignore = 
    E203,
    E501,
    W503,
    F401
per-file-ignores =
    __init__.py:F401
